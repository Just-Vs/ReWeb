<div class="table">
  <table>
    <tr>
      <th>Username</th>
      <th>Points</th>
      <th>Score</th>
    </tr>
    <% scores.forEach(score => {%>
      <tr>
        <td><%=score.name %></td>
        <td><%=score.points %></td>
        <td><%=score.type %></td>
      </tr>
    <% }) %>

  </table>
</div>

<form action="/logout?_method=DELETE" method="POST">
  <button type="button" name="button">logOut</button>
</form>
