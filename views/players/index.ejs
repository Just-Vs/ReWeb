<h2 class="header">Players</h2>
<form action="/players" method="GET">
  <div>
  <label>Name</label>
  <input type="text" name="name" value="<%= searchOptions.name %>">
  <div>
  <button type="submit">search</button>
</form>
<br>
<% players.forEach(player => {%>
  <div><%=player.name %></div>
  <div><%=player.email %></div>
  <% }) %>
